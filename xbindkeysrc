"/usr/bin/urxvtc"
  Alt + t
"/usr/bin/chromium"
  Alt + b
"/usr/bin/amixer sset Master on; amixer sset Master 5%+"
  XF86AudioRaiseVolume
"/usr/bin/amixer sset Master 5%-"
  XF86AudioLowerVolume
"/usr/bin/amixer sset Master toggle"
  XF86AudioMute
"/usr/bin/kbdlight down"
  XF86KbdBrightnessDown
"/usr/bin/kbdlight up"
  XF86KbdBrightnessUp
"/usr/bin/xbacklight +5"
  XF86MonBrightnessUp
"/usr/bin/xbacklight -5"
  XF86MonBrightnessDown
"/usr/local/bin/bluetooth_toggle --toggle"
  XF86LaunchB
"$DOTFILES/bin/toggle_external_display.rb"
  XF86LaunchA
"$DOTFILES/bin/toggle_external_display.rb"
  XF86Display
